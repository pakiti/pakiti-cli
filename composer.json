{
    "name": "pakiti/pakiti-cli",
    "description": "Create new projects with ease",
    "keywords": [
        "packages",
        "cli"
    ],
    "homepage": "https://pakiti.github.io/",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pakiti/pakiti-cli/issues",
        "source": "https://github.com/pakiti/pakiti-cli"
    },
    "authors": [
        {
            "name": "Alpha Olomi",
            "email": "alphaolomi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "nunomaduro/termwind": "^1.14",
        "laravel/pint": "^1.2",
        "laravel-zero/framework": "^10.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^2.34.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/pakiti"
    ]
}
